{
  "version": 1,
  "defaultEngine": "docling",
  "engines": ["docling", "pymupdf4llm"],
  "pymupdf4llm": {
    "requireLayout": true,
    "toMarkdown": {
      "write_images": false,
      "embed_images": false,
      "dpi": 150,
      "page_chunks": true,
      "extract_words": true,
      "force_text": true,
      "show_progress": false,
      "margins": 0,
      "table_strategy": "lines_strict",
      "graphics_limit": 0,
      "ignore_code": false
    }
  }
}
